<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
 
   <servlet>
   	<servlet-name>HomeServlet</servlet-name>
   	<display-name>HomeServlet</display-name>
   	<description></description>
   	<servlet-class>HomeServlet</servlet-class>
   </servlet>
   <servlet>
   	<servlet-name>TodoServlet</servlet-name>
   	<display-name>TodoServlet</display-name>
   	<description></description>
   	<servlet-class>TodoServlet</servlet-class>
   </servlet>
   <servlet>
   	<servlet-name>DisplayServlet</servlet-name>
   	<display-name>DisplayServlet</display-name>
   	<description></description>
   	<servlet-class>DisplayServlet</servlet-class>
   </servlet>
   <servlet>
   	<servlet-name>InsertServlet</servlet-name>
   	<display-name>InsertServlet</display-name>
   	<description></description>
   	<servlet-class>InsertServlet</servlet-class>
   </servlet>
   
  <servlet-mapping>
  	<servlet-name>HomeServlet</servlet-name>
  	<url-pattern>/HomeServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>TodoServlet</servlet-name>
  	<url-pattern>/TodoServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DisplayServlet</servlet-name>
  	<url-pattern>/DisplayServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DisplayServlet</servlet-name>
  	<url-pattern>/AddTodo</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>InsertServlet</servlet-name>
  	<url-pattern>/InsertServlet</url-pattern>
  </servlet-mapping>

  
</web-app>
